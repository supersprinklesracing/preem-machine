{
  "name": "@preem-machine/main",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "{workspaceRoot}/apps/main/jest.config.ts",
        "passWithNoTests": true,
        "color": false,
        "colors": false,
        "forceExit": true
      },
      "configurations": {
        "node": {
          "jestConfig": "{workspaceRoot}/apps/main/jest.node.config.ts",
          "outputs": ["{workspaceRoot}/coverage/{projectRoot}-node"]
        }
      }
    }
  }
}
